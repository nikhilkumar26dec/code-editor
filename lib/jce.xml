<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">

<RSyntaxTheme version="1.0">

   <!-- Omitting baseFont will use a system-appropriate monospaced. -->
   <!--<baseFont family="..." size="12"/>-->
   
   <!--  General editor colors. -->
   <background color="272822"/>
   <caret color="FFFFFF"/>
   <selection useFG="false" bg="49483E" roundedEdges="false"/>
   <currentLineHighlight color="49483E" fade="false"/>
   <marginLine fg="394448"/>
   <markAllHighlight color="6b8189"/> <!-- TODO: Fix me -->
   <markOccurrencesHighlight color="5b7179" border="false"/>
   <matchedBracket fg="DBE0CC" highlightBoth="true" animate="false"/>
   <hyperlinks fg="a082bd"/>
   <secondaryLanguages>
      <language index="1" bg="373833"/>
      <language index="2" bg="373833"/>
      <language index="3" bg="373833"/>
   </secondaryLanguages>
   
   <!-- Gutter styling. -->
   <gutterBorder color="272822"/>
   <lineNumbers fg="8F908A"/>
   <foldIndicator fg="8F908A" iconBg="272822"/>
   <iconRowHeader activeLineRange="3399ff"/>
   
   <!-- Syntax tokens. -->
   <tokenStyles>
      <style token="IDENTIFIER" fg="E0E2E4"/>
      <style token="RESERVED_WORD" fg="f92672"/>
      <style token="RESERVED_WORD_2" fg="f92672"/>
      <style token="ANNOTATION" fg="E8E2B7"/>
      <style token="COMMENT_DOCUMENTATION" fg="676E54"/>
      <style token="COMMENT_EOL" fg="676E54"/>
      <style token="COMMENT_MULTILINE" fg="676E54"/>
      <style token="COMMENT_KEYWORD" fg="ae9fbf"/>
      <style token="COMMENT_MARKUP" fg="ae9fbf"/>
      <style token="FUNCTION" fg="66D9EF" italic="true"/>
      <style token="DATA_TYPE" fg="66D9EF" italic="true"/>
      <style token="LITERAL_BOOLEAN" fg="9880E9"/>
      <style token="LITERAL_NUMBER_DECIMAL_INT" fg="9880E9"/>
      <style token="LITERAL_NUMBER_FLOAT" fg="9880E9"/>
      <style token="LITERAL_NUMBER_HEXADECIMAL" fg="9880E9"/>
      <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="9880E9"/>
      <style token="LITERAL_CHAR" fg="EC7600"/>
      <style token="LITERAL_BACKQUOTE" fg="9880E9"/>
      <style token="MARKUP_TAG_DELIMITER" fg="FFFFFF"/>
      <style token="MARKUP_TAG_NAME" fg="f92672" bold="false"/>
      <style token="MARKUP_TAG_ATTRIBUTE" fg="99DC2A"/>
      <style token="MARKUP_TAG_ATTRIBUTE_VALUE" fg="E6DB74"/>
      <style token="MARKUP_COMMENT" fg="676E54"/>
      <style token="MARKUP_DTD" fg="A082BD"/>
      <style token="MARKUP_PROCESSING_INSTRUCTION" fg="A082BD"/>
      <style token="MARKUP_CDATA" fg="d5e6f0"/>
      <style token="MARKUP_CDATA_DELIMITER" fg="ae9fbf"/>
      <style token="MARKUP_ENTITY_REFERENCE" fg="678CB1"/>
      <style token="OPERATOR" fg="F92672"/>
      <style token="PREPROCESSOR" fg="A082BD"/>
      <style token="REGEX" fg="d39745"/>
      <style token="SEPARATOR" fg="E8E2B7"/>
      <style token="VARIABLE" fg="ae9fbf" bold="true"/>
      <style token="WHITESPACE" fg="E0E2E4"/>
      
      <style token="ERROR_IDENTIFIER" fg="E0E2E4" bg="04790e"/>
      <style token="ERROR_NUMBER_FORMAT" fg="E0E2E4" bg="04790e"/>
      <style token="ERROR_STRING_DOUBLE" fg="E0E2E4" bg="04790e"/>
      <style token="ERROR_CHAR" fg="E0E2E4" bg="04790e"/>
   </tokenStyles>

</RSyntaxTheme>